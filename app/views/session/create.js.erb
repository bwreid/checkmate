<% if @auth %>
$('#login-form').slideUp(400);
$('.nav-wrapper').empty().append('<%=j render partial: 'shared/nav' %>');
$('.bank-navigation').empty().append('<%=j render partial: 'banks/banks' %>');
$('.galaxy').animate({height:250}, 'fast');
<% else %>
$('#login-form').empty().append('<%=j render partial: 'form' %>');
<% end %>